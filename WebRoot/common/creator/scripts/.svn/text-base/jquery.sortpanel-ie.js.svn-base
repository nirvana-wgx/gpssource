/* ================================================================ */
(function($) {
/* ================================================================ */
	$(function() {
		$('.sortpanel table th:last-child').addClass('last_child');				
		$('.sortpanel table td:last-child').addClass('last_child');
		$('.sortpanel table tr:last-child td').addClass('last_tr');
		$('.sortpanel table tr:even td').addClass('td_odd');
		$('.sortpanel_status li:last-child div').addClass('li_last');
		$('.sortpanel_list li:last-child').addClass('li_last');
	});
/* ================================================================ */
})( jQuery );
/* ================================================================ */