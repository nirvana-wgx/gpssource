<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC   
    "-//mybatis.org//DTD Mapper 3.0//EN"  
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
 <mapper namespace="com.gps.vehicledistr.dao.VehicleDistrDao">  
      
      <select id="findAll"  resultType="List" parameterType="VehicleInf">
      		select t.*  from account t where 1=1   order by  aid  limit #{start},#{end}
      </select>
      
 </mapper> 