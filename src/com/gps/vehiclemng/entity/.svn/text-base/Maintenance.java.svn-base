package com.gps.vehiclemng.entity;

import java.util.Date;

public class Maintenance implements java.io.Serializable {

	/**
	 * 车辆保养
	 */
	private static final long serialVersionUID = 1L;
	
	private String maid;         //id
	private String vehicleid;    //车辆编号
	private String fitting;      //保养配件
	private Date selltime;      //销售时间
	private Date expiretime;    //预警时间
	private Date maintenancetime; //保养时间
	private String action;       //保养动作      '未保养'    '完成保养'
	private String isovertime;   //是否超时      0:未超时   1:超时
	private String state;        //设备状态   
	private String creater;      //创建者
	private Date createtime;     //创建时间
	private String remark;       //备注
	private Date finishtime;     //保养完成时间
	
	public Maintenance() {
		
	}
	
	public Maintenance(String maid, String vehicleid, String fitting, Date selltime,
			Date expiretime, Date maintenancetime, String action, String isovertime, 
			String state, String creater, Date createtime, String remark ) {
		this.maid = maid;
		this.vehicleid = vehicleid;
		this.fitting = fitting;
		this.selltime = selltime;
		this.expiretime = expiretime;
		this.maintenancetime = maintenancetime;
		this.action = action;
		this.isovertime = isovertime;
		this.state = state;
		this.creater = creater;
		this.createtime = createtime;
		this.remark = remark;
	}
	
	public String getMaid() {
		return maid;
	}
	public void setMaid(String maid) {
		this.maid = maid;
	}
	public String getVehicleid() {
		return vehicleid;
	}
	public void setVehicleid(String vehicleid) {
		this.vehicleid = vehicleid;
	}
	public String getFitting() {
		return fitting;
	}
	public void setFitting(String fitting) {
		this.fitting = fitting;
	}
	public Date getSelltime() {
		return selltime;
	}
	public void setSelltime(Date selltime) {
		this.selltime = selltime;
	}
	public Date getExpiretime() {
		return expiretime;
	}
	public void setExpiretime(Date expiretime) {
		this.expiretime = expiretime;
	}
	public Date getMaintenancetime() {
		return maintenancetime;
	}
	public void setMaintenancetime(Date maintenancetime) {
		this.maintenancetime = maintenancetime;
	}
	public String getAction() {
		return action;
	}
	public void setAction(String action) {
		this.action = action;
	}
	public String getIsovertime() {
		return isovertime;
	}
	public void setIsovertime(String isovertime) {
		this.isovertime = isovertime;
	}
	public String getState() {
		return state;
	}
	public void setState(String state) {
		this.state = state;
	}
	public String getCreater() {
		return creater;
	}
	public void setCreater(String creater) {
		this.creater = creater;
	}
	public Date getCreatetime() {
		return createtime;
	}
	public void setCreatetime(Date createtime) {
		this.createtime = createtime;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}

	public Date getFinishtime() {
		return finishtime;
	}

	public void setFinishtime(Date finishtime) {
		this.finishtime = finishtime;
	}
	

}
